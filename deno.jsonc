{
    "tasks": {
        "dev": "deno run --check --allow-all main.ts",
        "compile": "deno compile --output di --check --allow-all main.ts",
        "test": "deno test --allow-all --check "
    },
    "fmt": {
        "indentWidth": 4,
        "semiColons": true,
        "singleQuote": false,
        "lineWidth": 200
    },
    "imports": {
        "command-exists": "npm:command-exists@~1.2.9",
        "@std/assert": "jsr:@std/assert@1"
    }
}
